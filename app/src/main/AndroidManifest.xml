<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="renato.clipto">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Criptomoedas"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity android:name="renato.clipto.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="renato.clipto.CenterTabsActivity" />
        <activity android:name="renato.clipto.CustomTabsActivity" />
        <activity android:name="renato.clipto.FixedTabsActivity" />
        <activity android:name="renato.clipto.ScrollableTabsActivity" />
        <activity android:name="renato.clipto.IconsAndTextActivity" />
        <activity android:name="renato.clipto.OnlyIconsTabsActivity" />
    </application>

</manifest>